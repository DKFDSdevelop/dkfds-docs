{% from "components/table/table.njk" import table as table %}

<main class="container page-container" id="main-content">
    <div class="row mb-905">
        <div class="col-12">
            <h1 class="h2 mt-0">test-tables-2</h1>
        </div>
    </div>
    <div class="row mb-905">
        <div class="col-12">
            <p>Klasser</p>
            {{ 
                table({
                    "classes": {
                        "outer": "mt-8",
                        "inner": "w-percent-md-70"
                    },
                    "thead": [
                        { "headerContent": "First column", "classes": "pt-1" },
                        { "headerContent": "Second column", "classes": "pt-1" },
                        { "headerContent": "Third column", "classes": "pt-1" }
                    ],
                    "tbody": [
                        [
                            { "content": "First row, first cell", "classes": "pt-3" },
                            { "content": "First row, second cell", "classes": "pt-3" },
                            { "content": "First row, third cell", "classes": "pt-3" }
                        ],
                        [
                            { "content": "Second row, first cell", "classes": "pt-5" },
                            { "content": "Second row, second cell", "classes": "pt-5" },
                            { "content": "Second row, third cell", "classes": "pt-5" }
                        ],
                        [
                            { "content": "Third row, first cell", "classes": "pt-8" },
                            { "content": "Third row, second cell", "classes": "pt-8" },
                            { "content": "Third row, third cell", "classes": "pt-8" }
                        ]
                    ]
                })
            }}
        </div>
    </div>
</main>