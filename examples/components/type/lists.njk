{% from "components/type/lists.njk" import ul_list as ul_list %}
{% from "components/type/lists.njk" import ol_list as ol_list %}

<div class="container">
  <div class="row">
    <div class="col-12 col-sm-12 col-md-6 col-lg-6">
      <p class="h4">Unordered list</p>
        <ul>
          {{ ul_list(lists.list_ul, 'sub_list') }}
        </ul>
    </div>
    <div class="col-12 col-sm-12 col-md-6 col-lg-6">
      <p class="h4">Ordered list</p>
        <ol>
          {{ ol_list(lists.list_ol, 'sub_list') }}
        </ol>
    </div>
  </div>
</div>