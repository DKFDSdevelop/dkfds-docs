    {{datoinput(dateObject.label, dateObject.hint, dateObject.day, dateObject.month, dateObject.year, dateObject.tooltip, dateObject.tooltipTitle, dateObject.datepicker, dateObject.error, dateObject.dayValue, dateObject.monthValue, dateObject.yearValue, dateObject.id, dateObject.formClass)}}

</form>

