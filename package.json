{
  "name": "dkfds-docs",
  "version": "3.0.0",
  "description": "Website and documentation on using the Frontend Styleguide",
  "mode": "none",
  "scripts": {
    "build": "webpack --mode production --color",
    "dev-build": "webpack --mode development --color",
    "dev-build-watch": "webpack --mode development --color --watch",
    "build-html": "gulp html",
    "postinstall": "bundle",
    "prestart": "npm run build",
    "start": "bundle exec jekyll serve --incremental --trace --config _config.yml,_config.dev.yml",
    "publish": "npm run build && bundle exec jekyll build --config _config.yml && gulp publish",
    "publish-dev": "npm run build && bundle exec jekyll build --config _config.yml,_config.dev.yml && gulp publish",
    "deploy": "bundle exec jekyll build --config _config.yml,_config.dev.yml && gulp publish"
  },
  "dependencies": {
    "core-js": "^3.21.1",
    "dkfds": "^7.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^6.6.0",
    "easy-pdf-merge": "^0.2.0",
    "event-stream": "^4.0.1",
    "file-loader": "6.2.0",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-count": "^1.0.0",
    "gulp-data": "^1.3.1",
    "gulp-eslint": "^6.0.0",
    "gulp-filter": "^7.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-if": "^3.0.0",
    "gulp-jsbeautifier": "^3.0.1",
    "gulp-modify-file": "^1.0.1",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-remote-src": "^0.4.4",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-run-command": "0.0.10",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-spawn-mocha": "^6.0.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.0",
    "gulp4-run-sequence": "^1.0.1",
    "jquery": "^3.3.1",
    "jshint": "^2.13.4",
    "mini-css-extract-plugin": "^2.5.3",
    "node-notifier": "^10.0.1",
    "node-sass": "^6.0.0",
    "postcss-csso": "^5.0.0",
    "postcss-discard-comments": "^5.0.3",
    "postcss-loader": "^6.2.1",
    "puppeteer": "^11.0.0",
    "sass": "^1.49.7",
    "sass-loader": "^12.6.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
