{
  "name": "dkfds-docs",
  "version": "3.0.0",
  "description": "Website and documentation on using the Frontend Styleguide",
  "scripts": {
    "build": "webpack --color ",
    "dev-build": "webpack --mode development --color",
    "dev-build-watch": "webpack --mode development --color --watch",
    "build-html": "gulp html",
    "postinstall": "bundle",
    "prestart": "npm run build",
    "start": "bundle exec jekyll serve --config _config.yml,_config.dev.yml",
    "publish": "npm run build && bundle exec jekyll build --config _config.yml && gulp publish",
    "publish-dev": "npm run build && bundle exec jekyll build --config _config.yml,_config.dev.yml && gulp publish",
    "deploy": "bundle exec jekyll build --config _config.yml,_config.dev.yml && gulp publish"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "dkfds": "^5.0.0",
    "easy-pdf-merge": "^0.2.0",
    "gulp-flatten": "^0.4.0",
    "jquery": "^3.3.1",
    "puppeteer": "^1.12.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "event-stream": "^4.0.1",
    "file-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-if": "^3.0.0",
    "gulp-count": "^1.0.0",
    "gulp-data": "^1.3.1",
    "gulp-jsbeautifier": "^2.1.2",
    "gulp-modify-file": "^1.0.1",
    "gulp-nunjucks-render": "^2.2.2",
    "gulp-plumber": "^1.2.0",
    "gulp-remote-src": "^0.4.3",
    "gulp-rename": "^1.4.0",
    "gulp-run-command": "0.0.9",
    "gulp-util": "^3.0.8",
    "mini-css-extract-plugin": "^0.4.4",
    "node-notifier": "^5.3.0",
    "node-sass": "^4.10.0",
    "postcss-loader": "^3.0.0",
    "run-sequence": "^2.2.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
