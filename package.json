{
  "name": "dkfds-docs",
  "version": "3.0.0",
  "description": "Website and documentation on using the Frontend Styleguide",
  "mode": "none",
  "scripts": {
    "build": "webpack --mode production --color",
    "dev-build": "webpack --mode development --color",
    "dev-build-watch": "webpack --mode development --color --watch",
    "build-html": "gulp html",
    "postinstall": "bundle",
    "start": "bundle exec jekyll serve --incremental --trace --config _config.yml,_config.dev.yml",
    "publish": "npm run build && bundle exec jekyll build --config _config.yml && gulp publish",
    "publish-dev": "npm run build && bundle exec jekyll build --config _config.yml,_config.dev.yml && gulp publish",
    "deploy": "bundle exec jekyll build --config _config.yml,_config.dev.yml && gulp publish"
  },
  "dependencies": {
    "dkfds": "~10.2.0"
  },
  "devDependencies": {
    "@babel/core": "~7.26.0",
    "@babel/preset-env": "~7.26.0",
    "autoprefixer": "~10.4.20",
    "babel-loader": "~9.2.1",
    "copy-webpack-plugin": "~12.0.2",
    "css-loader": "~7.1.2",
    "file-loader": "~6.2.0",
    "fs-extra": "~11.3.0",
    "gulp": "~4.0.2",
    "gulp-cli": "~2.3.0",
    "gulp-count": "~1.0.0",
    "gulp-data": "~1.3.1",
    "gulp-eslint": "~6.0.0",
    "gulp-flatten": "~0.4.0",
    "gulp-if": "~3.0.0",
    "gulp-jsbeautifier": "~3.0.1",
    "gulp-modify-file": "~1.0.1",
    "gulp-nunjucks-render": "~2.2.3",
    "gulp-plumber": "~1.2.1",
    "gulp-remote-src": "~0.4.4",
    "gulp-rename": "~2.0.0",
    "gulp-run-command": "~0.0.10",
    "gulp-sass": "~5.1.0",
    "gulp-sourcemaps": "~3.0.0",
    "gulp-uglify": "~3.0.0",
    "gulp4-run-sequence": "~1.0.1",
    "jquery": "~3.7.1",
    "jshint": "~2.13.6",
    "mini-css-extract-plugin": "~2.9.2",
    "node-notifier": "~10.0.1",
    "postcss-discard-comments": "~5.1.2",
    "postcss-loader": "~8.1.1",
    "puppeteer": "~23.11.1",
    "sass": "~1.83.1",
    "sass-loader": "~16.0.4",
    "terser-webpack-plugin": "~5.3.11",
    "webpack": "~5.97.1",
    "webpack-cli": "~6.0.1",
    "webpack-remove-empty-scripts": "~1.0.4"
  }
}
