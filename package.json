{
  "name": "dkfds-docs",
  "version": "3.0.0",
  "description": "Website and documentation on using the Frontend Styleguide",
  "scripts": {
    "build": "webpack --color ",
    "dev-build": "webpack --mode development --color",
    "dev-build-watch": "webpack --mode development --color --watch",
    "build-html": "gulp html",
    "postinstall": "bundle",
    "prestart": "npm run build",
    "start": "bundle exec jekyll serve --incremental --trace --config _config.yml,_config.dev.yml",
    "publish": "npm run build && bundle exec jekyll build --config _config.yml && gulp publish",
    "publish-dev": "npm run build && bundle exec jekyll build --config _config.yml,_config.dev.yml && gulp publish",
    "deploy": "bundle exec jekyll build --config _config.yml,_config.dev.yml && gulp publish"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "dkfds": "^7.0.0",
    "easy-pdf-merge": "^0.2.0",
    "jquery": "^3.3.1",
    "puppeteer": "^1.12.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "event-stream": "^4.0.1",
    "file-loader": "6.2.0",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-count": "^1.0.0",
    "gulp-data": "^1.3.1",
    "gulp-eslint": "^5.0.0",
    "gulp-filter": "^6.0.0",
    "gulp-flatten": "^0.4.0",
